{
  "expo": {
    "name": "Surely",
    "slug": "surely",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "4",
      "bundleIdentifier": "com.surelytodo",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "web": {
      "name": "Surely",
      "themeColor": "#4caf50",
      "favicon": "./assets/favicon.png"
    },
    "description": "Native and web client for surelytodo.com todo list app",
    "githubUrl": "https://github.com/CodingItWrong/surely-expo/"
  }
}
